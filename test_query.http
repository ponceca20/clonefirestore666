### Test 1: Query que deber√≠a encontrar el producto MobileGenius (precio 999.99)
POST http://localhost:3030/api/v1/organizations/new-org-1749766807/projects/new-proj-from-postman/databases/Database-2026/query/productos
Content-Type: application/json

{
  "from": [
    {
      "collectionId": "products"
    }
  ],
  "where": {
    "compositeFilter": {
      "op": "AND",
      "filters": [
        {
          "fieldFilter": {
            "field": {
              "fieldPath": "price"
            },
            "op": "GREATER_THAN_OR_EQUAL",
            "value": {
              "doubleValue": 30.00
            }
          }
        },
        {
          "fieldFilter": {
            "field": {
              "fieldPath": "price"
            },
            "op": "LESS_THAN_OR_EQUAL",
            "value": {
              "doubleValue": 1200.00
            }
          }
        },
        {
          "fieldFilter": {
            "field": {
              "fieldPath": "brand"
            },
            "op": "EQUAL",
            "value": {
              "stringValue": "MobileGenius"
            }
          }
        }
      ]
    }
  }
}

